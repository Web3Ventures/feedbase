    "@feedbase/tsconfig": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.3.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "turbo": "^2.0.12"
  },
  "packageManager": "pnpm@8.6.2"
